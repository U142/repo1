<?xml version="1.0"?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="ConsoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
      </listeners>
    </trace>
  </system.diagnostics>
  <appSettings>
    <!--General-->
    <add key="ParsePath" value="C:\ums\var\cb"/>
    <add key="CPUAffinity" value="0"/>
    <add key="Retries" value="2"/>
